
@{
    Layout = "";
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Entrancecaoch.com is an online entrance coaching platform for Medical NEET (UG), Engineering IIT JEE Main, JEE Advanced, Class 6 to 12, NTSE, KVPY Olympiad & Other Competitive Exams.">
    <meta name="author" content="entrancecoach.com">
    <title>Support - entrancecoach.com</title>
    <!-- Preloader -->
    <link href="~/Content/Site.css" rel="stylesheet" />

    <!-- Favicon -->
    <link rel="icon" href="~/Theme/favicon.png" type="image/png">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Theme/assets/libs/fortawesome/fontawesome-free/css/all.min.css">
    <!-- Quick CSS -->
    <link rel="stylesheet" href="~/Theme/assets/quick-website.css" id="stylesheet">
</head>

<body>
    <a href="~/Home/" class="btn btn-neutral btn-icon-only rounded-circle position-absolute left-4 top-4 d-none d-lg-inline-flex" data-toggle="tooltip" data-placement="right" title="" data-original-title="Go back">
        <span class="btn-inner--icon">
            <i data-feather="arrow-left"></i>
        </span>
    </a>
    <section class="slice slice-lg">
        <div class="container">
            <div class="row justify-content-center text-center">               
                <div class="col-lg-8 col-md-10">
                    <h6 class="display-1 mb-3 font-weight-600 text-warning">Ooops!</h6>

                    @if (ViewBag.ErrorMessage != null)
                    {
                <p class="lead text-lg mb-5 text-danger">@Html.Raw(ViewBag.ErrorMessage.Replace(System.Environment.NewLine, "<br/>"))</p>
                    }
                    else
                    {
                        <p class="lead text-lg mb-5">The page you are looking for could not be found.</p>
                    }

                    <a href="Dashboard" class="btn btn-dark btn-icon hover-translate-y-n3">
                        <span class="btn-inner--icon">
                            <i data-feather="home"></i>
                        </span>
                        <span class="btn-inner--text">Return home</span>
                    </a>
                </div>
            </div>
        </div>
    </section>

    <section class="slice slice-lg">
        <div class="container">
            <div class="row mb-5 justify-content-center text-center">
                <div class="col-lg-8 col-md-10">
                    <h2 class="mt-4">Need personal assistance?</h2>
                    <div class="mt-2">
                        <p class="lead lh-180">If you still didn't find an answer you can always contact us</p>
                    </div>
                </div>
            </div>
            <div class="row justify-content-center">
                <div class="col-md-5">
                    <div class="card">
                        <div class="card-body p-5 text-center">
                            <div class="pb-4">
                                <img src="~/Theme/assets/img/svg/icons/Call_Out.svg" style="height:40px">
                            </div>
                            <h5>0091 9496611912</h5><p class="mt-2 mb-0">Our support team is here for you.</p>
                        </div>
                    </div>
                </div>
                <div class="col-md-5">
                    <div class="card">
                        <div class="card-body p-5 text-center">
                            <div class="pb-4">
                                <img src="~/Theme/assets/img/svg/icons/Email.svg" style="height:40px">
                            </div><h5><a href="mailto:support@entrancecoach.com">support@entrancecoach.com</a></h5><p class="mt-2 mb-0">You'll get a reply in 4 to 12 hours.</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Core JS  -->
    <script src="~/Theme/assets/libs/jquery/dist/jquery.min.js"></script>
    <script src="~/Theme/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/Theme/assets/libs/feather-icons/dist/feather.min.js"></script>
    <!-- Quick JS -->

    <script src="~/Theme/assets/quick-website.js"></script>
    <!-- Feather Icons -->
    <script>
        feather.replace({
            'width': '1em',
            'height': '1em'
        })
    </script>
</body>

</html>