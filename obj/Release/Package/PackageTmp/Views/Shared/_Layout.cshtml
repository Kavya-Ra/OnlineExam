    <!-- Quick CSS -->
    <link rel="stylesheet" href="~/Theme/assets/quick-website.css" id="stylesheet">
</head>

<body>
    <!-- Preloader -->
    @*<div class="preloader">
            <div class="spinner-border text-primary" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>*@

