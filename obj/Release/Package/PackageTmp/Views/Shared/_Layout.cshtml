    <link rel="stylesheet" href="~/Theme/assets/libs/fortawesome/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="~/Theme/assets/libs/flatpickr/dist/flatpickr.min.css">
    <link rel="stylesheet" href="~/Theme/assets/libs/select2/dist/css/select2.min.css">
